{
  "name": "nuxt-fb-tailwind-ts-template",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "lint:eslint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:prettier": "prettier . --check --ignore-path .gitignore",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:style",
    "format": "yarn lint:eslint --fix && yarn lint:prettier --write && yarn lint:style --fix",
    "test:unit": "jest",
    "test:e2e": "jest --config jest.e2e.config.js --runInBand",
    "testServer": "yarn build -q && yarn start -p 4000"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "core-js": "^3.6.5",
    "firebase": "^7.21.1",
    "nuxt": "^2.14.5"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.5",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config-typescript": "^3.0.0",
    "@nuxtjs/tailwindcss": "^3.0.2",
    "@types/expect-puppeteer": "^4.4.3",
    "@types/jest-environment-puppeteer": "^4.3.2",
    "@types/puppeteer": "^3.0.2",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "firebase-tools": "^8.11.2",
    "jest": "^26.4.2",
    "jest-puppeteer": "^4.4.0",
    "prettier": "^2.1.1",
    "puppeteer": "^5.3.1",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.3.0",
    "vue-jest": "^3.0.4"
  }
}
